loop = "Y"
compnum = 50
count = 1


while loop == "Y":
  guess = int(input("Guess the number"))
  
  if guess == 50:
    print("Well done, you guessed it correct")
    count = count + 1
    print("It took you this many attempts:" , count)
    break
  
  elif guess < 50:
    print("Too low")
    count = count + 1
  
  elif guess > 50:
    print("too high")
    count = count + 1
