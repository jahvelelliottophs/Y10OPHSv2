invite=[]
  
for inv in range (1):
  List=input("who do you want to invite") 
  List2=input("who do you want to invite") 
  List3=input("who do you want to invite") 
  
  

invite.append(List)
invite.append(List2)
invite.append(List3)
print(invite)

ans=input ("do you want to invite more people y or n>>>>")
if ans== "y":
  others=input("who else do you want to invite>>>")
  invite.append(others)
  print(invite)
  
else:
  print(invite)
  














































